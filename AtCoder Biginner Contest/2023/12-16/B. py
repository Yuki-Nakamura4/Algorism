S = input()

T = input()


str = {"A":1, "B":2, "C":3 ,"D":4, "E":5}

if abs(str[S[0]] -str[S[1]]) == 2 or abs(str[S[0]] -str[S[1]]) == 3:
    S_length = 2
else:
    S_length = 1

if abs(str[T[0]] -str[T[1]]) == 2 or abs(str[T[0]] -str[T[1]]) == 3:
    T_length = 2
else:
    T_length = 1

if S_length == T_length:
    print("Yes")
else:
    print("No")